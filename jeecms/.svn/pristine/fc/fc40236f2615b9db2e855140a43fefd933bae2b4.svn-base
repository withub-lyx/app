@charset "utf-8";
*{margin:0;padding:0;}

/*f1*/
.news,.gongg {
	width: 715px;
	padding: 5px;
	float: left;
	border: 1px solid #cccccc;
	}
	
.gongk,zhin {
	width: auto;
	padding: 5px;
	float: right;
	margin-left: 12px;
	border: 1px solid #cccccc;
	}

.gongk ul{
	display: block;
	list-style-type: none;
	text-align:center;
	
}
.gongk ul li{
	display: block;
	height: 84px;
	width: 100%;
	background-repeat: no-repeat;
	background-position: center center;
	margin-top: 3px;
	color: #711312;
	font-weight: bold;
	font-size: 20px;
	
}
.li_a{
	display: inline-block;
	height: 86px;
	width: 100%;
	background-repeat: no-repeat;
	background-position: center center;
	margin-top: 3px;
	color: #711312;
	font-weight: bold;
	font-size: 20px;
}
.gongk ul li a:hover {
	font-size:22px;
	color: #711312;
	}
.li1{
	background-image: url(li1.png);
}
.li2{
	background-image: url(li2.png);
}
.li3{
	background-image: url(li3.png);
}
