/*f3*/
.gongg {
	padding: 5px;
	height:300px;
	width:715px;
	}
.gongg a:hover{
	background-color:#1388ff;
	color:#fff;
}

.zhin {
	width: auto;
	float: right;
	margin-left: 12px;
	padding:5px;
	border: 1px solid #cccccc;
	border-top: 3px solid #1388ff;}
.zhin ul{
	display: block;
	list-style-type: none;
	text-align:center;
	
}
.zhin ul li{
	display: block;
	height: 28px;
	line-height:28px;
	width: 100%;
	background-repeat: no-repeat;
	background-position: center center;
	margin-top: 3px;
	color: #333;
	border: 1px solid #eee;
	background-color: #fff;
    background-image: -moz-linear-gradient(top, #d7d5d5, #fff);
    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#d7d5d5), to(#fff));
    background-image: -webkit-linear-gradient(top, #d7d5d5, #fff);
    background-image: -o-linear-gradient(top, #d7d5d5, #fff);
    background-image: linear-gradient(to bottom, #d7d5d5, #fff);
    background-repeat: repeat-x;	
}

.zhin ul li:hover {
	border: 1px solid #fbd18c;
	background-image: -moz-linear-gradient(top, #fbd18c, #fff);
    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#fbd18c), to(#fff));
    background-image: -webkit-linear-gradient(top, #fbd18c, #fff);
    background-image: -o-linear-gradient(top, #fbd18c, #fff);
    background-image: linear-gradient(to bottom, #fbd18c, #fff);
	}
