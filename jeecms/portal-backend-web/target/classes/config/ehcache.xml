<?xml version="1.0" encoding="UTF-8"?>
<ehcache name="es" dynamicConfig="true">

    <diskStore path="java.io.tmpdir"/>


    <cache name="wechat_ticket"
           maxEntriesLocalHeap="10000"
           overflowToDisk="false"
           eternal="false"
           diskPersistent="false"
           timeToLiveSeconds="5000"
           timeToIdleSeconds="0"
           statistics="true"/>

    <!--shior权限缓存-->
    <cache name="shiro-data"
           maxEntriesLocalHeap="10000"
           overflowToDisk="false"
           eternal="false"
           diskPersistent="false"
