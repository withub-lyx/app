           timeToIdleSeconds="0"
           statistics="true"/>



    <cache name="test"
           maxEntriesLocalHeap="10000"
           overflowToDisk="false"
           eternal="false"
           diskPersistent="false"
           timeToLiveSeconds="5"
           timeToIdleSeconds="0"
           statistics="true"/>




    <cache name="common_ehcache"
           maxEntriesLocalHeap="10000"
           overflowToDisk="false"
           eternal="false"
           diskPersistent="false"
           timeToLiveSeconds="0"
           timeToIdleSeconds="0"
           statistics="true"/>
<!--7200 -->
    <defaultCache
            maxEntriesLocalHeap="10000"
            overflowToDisk="false"
